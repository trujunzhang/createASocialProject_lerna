{
    "name": "@app/native-base",
    "version": "2.12.1",
    "homepage": "https://github.com/GeekyAnts/NativeBase#readme",
    "description": "Essential cross-platform UI components for React Native",
    "license": "Apache-2.0",
    "private": false,
    "main": "./src/index.ts",
    "types.pending": "dist/__types__/index.d.ts",
    "scripts": {
        "depenence.update": "ncu -u",
        "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
        "fix.current.pending": "yarn fix",
        "ts-lint:lib": "tslint -c tslint.json '**/*.{ts,tsx}' -e 'node_modules/**'",
        "test.lib": "jest --colors --coverage test",
        "test.current": "yarn test.lib",
        "build:reset": "rm -rf dist",
        "build:clean": "yarn build:reset",
        "build:types": "rm -rf dist/__types__ && tsc --emitDeclarationOnly -p tsconfig.lib.json",
        "build:js": "babel  --config-file ../../../../babel.config.js  src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
        "build:all": "yarn build:types && yarn build:js",
        "build-lib": "yarn build:clean && yarn build:all",
        "postinstall.pending": "yarn build-lib",
        "build.current.pending": "yarn build-lib"
    },
    "dependencies": {
        "create-react-class": "15.6.3",
        "@types/prop-types": "15.5.8",
        "prop-types": "15.6.2",
        "blueimp-md5": "2.10.0",
        "clamp": "1.0.1",
        "react-tween-state": "0.1.5",
        "tween-functions": "1.2.0",
        "react-dom": "^16.8.2",
        "@types/styled-components": "4.1.12",
        "styled-components": "4.1.3",
        "jest-expo": "32.0.0",
        "jest-react-native": "18.0.0",
        "metro-react-native-babel-preset": "0.53.1",
        "tslib": "1.9.3"
    },
    "devDependencies": {
        "identity-obj-proxy": "3.0.0",
        "jest-static-stubs": "0.0.1",
        "react-sketchapp": "3.0.0-beta.3",
        "@types/react-native": "0.57.27",
        "react": "16.5.0",
        "@types/node": "11.10.4",
        "@babel/plugin-proposal-decorators": "7.4.0",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "24.1.0",
        "@types/jest": "24.0.6",
        "jest": "24.1.0",
        "@babel/cli": "7.4.3",
        "@babel/core": "7.4.3",
        "@babel/runtime": "7.4.3",
        "@babel/polyfill": "7.4.3",
        "@babel/preset-env": "7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "7.3.3",
        "@babel/plugin-transform-runtime": "7.4.3",
        "@babel/plugin-proposal-class-properties": "7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "7.4.3",
        "@types/react-test-renderer": "16.8.1",
        "react-test-renderer": "16.7.0"
    },
    "peerDependencies": {
        "prop-types": "*",
        "react": "*",
        "react-dom": "*",
        "react-native": "*",
        "react-native-web": "*"
    },
    "rnpm": {
        "assets": [
            "Fonts"
        ]
    }
}
