{
  "name": "@app/native-base",
  "version": "0.6.0",
  "main": "build/lib/index.js",
  "typings": "build/lib/index.d.ts",
  "description": "Essential cross-platform UI components for React Native",
  "keywords": [
    "android",
    "ios",
    "react",
    "native",
    "react-native",
    "native-modules",
    "react-component",
    "navbar",
    "packager",
    "rnpm"
  ],
  "scripts": {
    "depenence.update": "ncu -u",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "ts-lint:lib": "tslint -c tslint.json '**/*.{ts,tsx}' -e 'node_modules/**'",
    "test.lib": "jest --env=node --colors --coverage test",
    "test.current.pending": "yarn test.lib",
    "build:reset": "rm -rf build",
    "build.clean": "yarn build:reset",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir build/lib --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "npm run build:types && npm run build:js",
    "build-lib": "yarn build.clean && yarn build:all",
    "build.current": "yarn build-lib",
    "postinstall": "yarn build-lib"
  },
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@app/native-base-shoutem-theme": "*",
    "@app/nativebase-theme-components": "*",
    "@app/native-base-variables": "*",
    "@app/tools": "*",
    "@types/prop-types": "15.5.8",
    "prop-types": "15.6.2",
    "blueimp-md5": "2.10.0",
    "clamp": "1.0.1",
    "react-tween-state": "0.1.5",
    "tween-functions": "1.2.0",
    "react-primitives": "*",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@types/react-sketchapp": "0.12.1",
    "react-sketchapp": "2.0.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@babel/runtime-corejs2": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@types/react-test-renderer": "16.0.3",
    "react-test-renderer": "16.7.0",
    "typescript": "3.3.3",
    "@types/node": "11.10.4",
    "@types/react": "16.4.8",
    "@types/react-native": "0.57.27",
    "react": "16.5.0"
  },
  "gitHead": "5bbeacc403ba97622703699132c55d8359344004",
  "homepage": "https://github.com/GeekyAnts/NativeBase#readme",
  "optionalDependencies": {},
