{
  "name": "app-sketch",
  "version": "2.0.0",
  "skpm": {
    "main": "appSketch.sketchplugin",
    "manifest": "./manifest.json"
  },
  "scripts": {
    "rm.assets": "rm -rf @types",
    "ready.assets.pending": "run-s  ready:*",
    "ready:appSharedComps": "cpx \"../appLibrary/appSharedComps/src/**/*.*\" ./src/shared-components",
    "build": "yarn typescript:once && skpm-build",
    "watch": "skpm-build --watch & yarn typescript",
    "render": "skpm-build --watch --run & yarn typescript",
    "render:once": "yarn typescript:once && skpm-build --run",
    "render:all": "yarn render:once && yarn render",
    "build:all": "yarn build && skpm-link",
    "postinstall.pending": "yarn build:all",
    "typescript.last": "tsc --watch",
    "typescript": "echo 'typescript watch'",
    "typescript:once": "tsc",
    "build:js": "yarn typescript:once",
    "typescript:once.error": "yarn build:js",
    "typescript:clean": "rm -rf ./.ts-compiled"
  },
  "dependencies": {
    "react-primitives": "*",
    "@app/shared-components": "*",
    "@types/react-dom": "16.0.11",
    "react-dom": "16.7.0",
    "@types/prop-types": "15.5.8",
    "prop-types": "15.6.2",
    "@types/react-native": "0.57.27",
    "@types/react": "16.4.8",
    "react-native": "0.42.3",
    "@skpm/builder": "0.4.0",
    "react": "16.3.2",
    "@types/react-sketchapp": "0.12.1",
    "react-sketchapp": "2.0.0"
  },
  "author": "Phuc Nguyen Hoang",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/runtime": "^7.3.1",
    "@babel/runtime-corejs2": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@types/react-test-renderer": "16.0.3",
    "react-test-renderer": "16.7.0",
    "typescript": "3.3.3",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/redux-mock-store": "1.0.0",
    "@types/sinon": "7.0.3",
    "enzyme": "3.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "iana-tz-data": ">=2017.1.0",
    "jest-enzyme": "7.0.1",
    "jest-expo": "32.0.0",
    "jest-react-native": "18.0.0",
    "npm-run-all": "4.1.5",
    "react-addons-shallow-compare": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-native-debugger-open": "0.3.17",
    "sinon": "7.2.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-react": "3.6.0"
  }
}