{
  "name": "@app/avatar",
  "version": "0.6.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "test.lib.pending": "jest --env=node --colors --coverage test",
    "test.current.pending": "yarn test.lib",
    "build:reset": "rm -rf dist",
    "build.clean": "yarn build:reset",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "npm run build:types && npm run build:js",
    "build-lib": "yarn build.clean && yarn build:all",
    "postinstall": "yarn build-lib"
  },
  "author": {
    "name": "trujunzhang",
    "email": "trujunzhang@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@types/react": "16.4.8",
    "react": "16.5.0",
    "@types/react-dom": "16.0.11",
    "react-dom": "16.7.0",
    "ts-md5": "1.2.4"
  },
  "devDependencies": {
    "@types/node": "11.10.4",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/runtime": "^7.3.4",
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime-corejs2": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@types/react-test-renderer": "16.0.3",
    "react-test-renderer": "16.7.0",
    "typescript": "3.3.3"
  }
}