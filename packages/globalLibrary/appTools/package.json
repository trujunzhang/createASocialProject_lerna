{
    "name": "@app/tools",
    "version": "0.6.0",
    "private": false,
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/__types__/index.d.ts",
    "scripts": {
        "depenence.update": "ncu -u",
        "rm.assets": "rm -rf @types",
        "ready.assets": "run-s  ready:*",
        "ready:definitive": "cpx \"../../globalLibrary/appDefinitive/@types/**/*.*\" ./@types",
        "ready:copy_static": "cpx \"../../appWeb/static/**/*.*\" ./static",
        "test.global": "jest --env=node --colors --coverage test",
        "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
        "build.copy.tld": "cpx \"./src/vendor/parse-domain/tld.js\" ./dist/vendor/parse-domain",
        "build:post": "yarn build.copy.tld",
        "build:reset": "rm -rf dist",
        "build:clean": "yarn build:reset",
        "build:types": "rm -rf dist/__types__ && tsc --emitDeclarationOnly -p tsconfig.lib.json",
        "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
        "build:all": "yarn build:types && yarn build:js",
        "build-lib": "yarn build:clean && yarn build:all && yarn build:post",
        "postinstall": "yarn build-lib",
        "build.current.pending": "yarn build:js"
    },
    "dependencies": {
        "@types/react": "16.7.7",
        "react": "16.5.0",
        "@types/react-native-vector-icons": "4.6.4",
        "i18next": "15.0.0",
        "i18next-browser-languagedetector": "3.0.1",
        "i18next-express-middleware": "1.7.2",
        "i18next-node-fs-backend": "2.1.1",
        "i18next-xhr-backend": "2.0.1",
        "react-i18next": "9.0.10",
        "camelcase": "5.2.0",
        "color": "1.0.3",
        "lodash": "4.14.2",
        "validate.js": "0.12.0",
        "route-parser": "0.0.5",
        "underscore": "1.9.1",
        "axios": "0.18.0",
        "slugify": "1.3.4",
        "moment": "2.24.0",
        "query-string": "6.2.0",
        "numeral": "2.0.6"
    },
    "devDependencies": {
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "@types/sinon": "5.0.2",
        "sinon": "6.1.5",
        "@types/route-parser": "0.1.1",
        "@types/underscore": "1.8.9",
        "@types/query-string": "6.2.0",
        "@app/mocks": "*",
        "@types/node": "11.10.4",
        "@babel/plugin-proposal-decorators": "7.4.0",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "24.1.0",
        "@types/jest": "24.0.6",
        "jest": "24.1.0",
        "@babel/cli": "7.4.3",
        "@babel/core": "7.4.3",
        "@babel/runtime": "7.4.3",
        "@babel/polyfill": "7.4.3",
        "@babel/preset-env": "7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "7.3.3",
        "@babel/plugin-transform-runtime": "7.4.3",
        "@babel/plugin-proposal-class-properties": "7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "7.4.3",
        "@types/react-test-renderer": "16.8.1",
        "react-test-renderer": "16.7.0"
    }
}