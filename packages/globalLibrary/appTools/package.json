{
  "name": "@app/tools",
  "version": "0.6.0",
  "private": false,
  "license": "Apache-2.0",
  "main": "build/lib/index.js",
  "author": {
    "name": "Stokedbits"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "depenence.update": "ncu -u",
    "rm.assets": "rm -rf @types",
    "test.global": "jest --env=node --colors --coverage test",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "build.reset": "rm -rf build",
    "build.clean": "yarn build.reset",
    "build:js": "tsc -p tsconfig.lib.json",
    "build.copy.tld": "cpx \"./src/vendor/parse-domain/tld.js\" ./build/lib/vendor/parse-domain",
    "build-lib": "yarn build.clean && yarn build:js  && yarn build.copy.tld",
    "build.current.pending": "yarn build-lib",
    "postinstall": "yarn build-lib"
  },
  "dependencies": {
    "color": "1.0.3",
    "lodash": "4.14.2",
    "validate.js": "0.12.0",
    "route-parser": "0.0.5",
    "underscore": "1.9.1",
    "axios": "0.18.0",
    "slugify": "1.3.4",
    "moment": "2.24.0",
    "query-string": "6.2.0",
    "numeral": "2.0.6"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "11.10.4",
    "@types/lodash": "4.14.120",
    "@types/route-parser": "0.1.1",
    "@types/underscore": "1.8.9",
    "@types/query-string": "6.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/runtime": "7.3.1",
    "@babel/runtime-corejs2": "7.3.1",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "7.3.3",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "typescript": "3.3.3"
  }
}