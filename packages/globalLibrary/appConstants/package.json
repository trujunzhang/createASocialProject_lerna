{
  "name": "@app/types",
  "version": "0.6.0",
  "private": false,
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Stokedbits"
  },
  "scripts": {
    "depenence.update": "ncu -u",
    "rm.assets": "rm -rf @types",
    "ready.assets": "run-s  ready:*",
    "ready:definitive": "cpx \"../appDefinitive/@types/**/*.*\" ./@types",
    "test.lib": "jest --env=node --colors --coverage test",
    "test.current.pending": "yarn test.lib",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "build:reset": "rm -rf dist",
    "build.clean": "yarn build:reset",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "npm run build:types && npm run build:js",
    "build-lib": "yarn build.clean && yarn build:all",
    "build.current.pending": "yarn build-lib",
    "postinstall": "yarn build-lib"
  },
  "dependencies": {
    "@app/avatar": "*",
    "@app/tools": "*",
    "@types/react": "16.4.8",
    "@types/underscore": "1.8.9",
    "underscore": "1.9.1",
    "validate.js": "0.12.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "11.10.4",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/runtime": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime-corejs2": "^7.1.2",
    "@babel/preset-env": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "typescript": "3.3.3"
  }
}