{
    "name": "react-primitives",
    "version": "0.6.0",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "src/typings/index.d.ts",
    "scripts": {
        "depenence.update": "ncu -u",
        "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
        "fix.current.pending": "yarn fix",
        "test.lib.pending": "jest --env=node --colors --coverage test",
        "test.current.pending": "yarn test.lib",
        "build:reset": "rm -rf dist",
        "build:clean": "yarn build:reset",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
        "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
        "build:all": "yarn build:types && yarn build:js",
        "build-lib": "yarn build:clean && yarn build:all",
        "postinstall.pending": "yarn build-lib",
        "build.current.pending": "yarn build:js"
    },
    "dependencies": {
        "@app/tools": "*",
        "animated": "0.2.2",
        "asap": "2.0.5",
        "create-react-class": "15.6.2",
        "flexibility": "2.0.1",
        "inline-style-prefixer": "4.0.2",
        "invariant": "2.2.1",
        "normalize-css-color": "1.0.1",
        "prop-types": "15.6.2",
        "react-timer-mixin": "0.13.3",
        "string-hash": "1.1.3"
    },
    "peerDependencies": {
        "react": "*",
        "react-art": "*",
        "react-dom": "*",
        "react-native-web": "*",
        "react-native": "*"
    },
    "devDependencies": {
        "react-native-web": "0.10.0",
        "react-art": "16.5.2",
        "react-sketchapp": "^3.0.0-beta.3",
        "@types/react-native": "0.57.27",
        "@types/react": "16.4.8",
        "react": "16.5.0",
        "@types/react-dom": "16.0.11",
        "react-dom": "16.8.2",
        "@types/prop-types": "15.5.8",
        "@types/node": "11.10.4",
        "@babel/plugin-proposal-decorators": "7.4.0",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "24.1.0",
        "@types/jest": "24.0.6",
        "jest": "24.1.0",
        "@babel/cli": "7.4.3",
        "@babel/core": "7.4.3",
        "@babel/runtime": "7.4.3",
        "@babel/polyfill": "7.4.3",
        "@babel/preset-env": "7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "7.3.3",
        "@babel/plugin-transform-runtime": "7.4.3",
        "@babel/plugin-proposal-class-properties": "7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "7.4.3",
        "@types/react-test-renderer": "16.8.1",
        "react-test-renderer": "16.7.0"
