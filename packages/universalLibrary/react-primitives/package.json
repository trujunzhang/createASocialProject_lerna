{
  "name": "react-primitives",
  "version": "0.6.0",
  "license": "MIT",
  "main": "build/lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "depenence.update": "ncu -u",
    "rm.assets": "rm -rf @types",
    "ready.assets": "run-s  ready:*",
    "ready:definitive": "cpx \"../../globalLibrary/appDefinitive/@types/**/*.*\" ./@types",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "fix.current": "yarn fix",
    "test.lib.pending": "jest --env=node --colors --coverage test",
    "test.current.pending": "yarn test.lib",
    "build:reset": "rm -rf build",
    "build.clean": "yarn build:reset",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir build/lib --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "npm run build:types && npm run build:js",
    "build-lib": "yarn build.clean && yarn build:all",
    "build.current.pending": "yarn build-lib",
    "postinstall": "yarn build-lib"
  },
  "dependencies": {
    "animated": "0.2.2",
    "asap": "2.0.5",
    "create-react-class": "15.6.2",
    "flexibility": "2.0.1",
    "inline-style-prefixer": "4.0.2",
    "invariant": "2.2.1",
    "normalize-css-color": "1.0.1",
    "prop-types": "15.6.2",
    "react-timer-mixin": "0.13.3",
    "string-hash": "1.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-art": "*",
    "react-dom": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@babel/runtime-corejs2": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@types/react-test-renderer": "16.0.3",
    "react-test-renderer": "16.7.0",
    "typescript": "3.3.3",
    "react-art": "16.5.2",
    "@types/react-sketchapp": "0.12.1",
    "react-sketchapp": "2.0.0",
    "@types/react-native": "0.57.27",
    "@types/react": "16.4.8",
    "react": "16.5.0",
    "@types/react-dom": "16.0.11",
    "react-dom": "16.8.2",
    "react-native-web": "0.10.0",
    "@types/prop-types": "15.5.8"
  }
}