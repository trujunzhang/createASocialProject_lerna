{
    "name": "@app/react-sketch-vector-icons",
    "version": "0.6.0",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "index.d.ts",
    "scripts": {
        "compile.bin:ready": "rm -rf scripts/generate",
        "compile.bin": "babel scripts/builder --out-dir scripts/generate --extensions \".ts,.tsx\"",
        "debug": "yarn compile.bin:ready && yarn compile.bin && node scripts/generate/index.js",
        "depenence.update": "ncu -u",
        "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
        "fix.current.pending": "yarn fix",
        "test.lib": "jest --env=node --colors --coverage test",
        "test.current.pending": "yarn test.lib",
        "build:reset": "rm -rf dist",
        "build:clean": "yarn build:reset",
        "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
        "build:all": "yarn build:js",
        "build-lib": "yarn build:clean && yarn build:all",
        "postinstall.pending": "yarn build-lib",
        "build.current.pending": "yarn build:js"
    },
    "dependencies": {
        "@app/tools": "*",
        "material-icons": "0.3.0",
        "@types/react-native-vector-icons": "4.6.4",
        "react-sketchapp": "^3.0.0-beta.3",
        "@types/react": "16.4.8",
        "react": "16.5.0",
        "@types/react-dom": "16.0.11",
        "react-dom": "16.8.2",
        "@types/prop-types": "15.5.8",
        "prop-types": "15.6.2",
        "react-art": "16.5.2"
    },
    "peerDependencies": {
        "react": "*",
        "react-art": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "camelcase": "5.0.0",
        "cheerio": "1.0.0-rc.2",
        "glob-promise": "3.4.0",
        "@jsbits/get-package-version": "^1.0.3",
        "ionicons": "4.5.5",
        "octicons": "8.5.0",
        "feather-icons": "4.19.0",
        "prettier": "1.13.7",
        "uppercamelcase": "3.0.0",
        "glob": "7.1.2",
        "identity-obj-proxy": "3.0.0",
        "@babel/plugin-proposal-decorators": "7.4.0",
        "babel-plugin-module-resolver": "3.2.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "24.1.0",
        "@types/jest": "24.0.6",
        "jest": "24.1.0",
        "@babel/cli": "7.4.3",
        "@babel/core": "7.4.3",
        "@babel/runtime": "7.4.3",
        "@babel/polyfill": "7.4.3",
        "@babel/preset-env": "7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "7.3.3",
        "@babel/plugin-transform-runtime": "7.4.3",
        "@babel/plugin-proposal-class-properties": "7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "7.4.3",
        "@types/react-test-renderer": "16.8.1",
        "react-test-renderer": "16.7.0"
    }
}