{
  "name": "@app/react-sketch-vector-icons",
  "version": "0.6.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "depenence.update": "ncu -u",
    "rm.assets": "rm -rf @types",
    "ready.assets": "run-s  ready:*",
    "ready:definitive": "cpx \"../../globalLibrary/appDefinitive/@types/**/*.*\" ./@types",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "fix.current": "yarn fix",
    "test.lib": "jest --env=node --colors --coverage test",
    "test.current": "yarn test.lib",
    "build.copy.jsons": "cpx \"./src/glyphmapsJS/*.json\" ./dist/glyphmapsJS",
    "postbuild": "yarn build.copy.jsons",
    "build:reset": "rm -rf dist",
    "build.clean": "yarn build:reset",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "npm run build:types && npm run build:js",
    "build-lib": "yarn build.clean && yarn build:all & yarn postbuild",
    "build.current": "yarn build-lib",
    "postinstall": "yarn build-lib",
    " ##  the following is generating automatically!  ## ": "",
    "generate.vectors": "run-s  generate:*",
    "compile.bin": "babel bin --out-dir generate --extensions \".ts,.tsx\"",
    "generate:feather": "yarn compile.bin && node ./generate/buildFeather",
    "generate:ionicons": "node ./bin/buildIonicons/make-svg-icons",
    "generate:octicons": "yarn compile.bin && node ./generate/buildocticons"
  },
  "dependencies": {
    "@app/tools": "*",
    "material-icons": "0.3.0",
    "@types/react-native-vector-icons": "4.6.4",
    "react-sketchapp": "2.0.0",
    "@types/react": "16.4.8",
    "react": "16.5.0",
    "@types/react-dom": "16.0.11",
    "react-dom": "16.8.2",
    "@types/prop-types": "15.5.8",
    "prop-types": "15.6.2",
    "react-art": "16.5.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-art": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@jsbits/get-package-version": "^1.0.3",
    "ionicons": "4.5.5",
    "octicons": "8.5.0",
    "feather-icons": "4.19.0",
    "prettier": "1.13.7",
    "uppercamelcase": "3.0.0",
    "cheerio": "1.0.0-rc.1",
    "glob": "7.1.2",
    "@types/node": "11.10.4",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime-corejs2": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@types/react-test-renderer": "16.0.3",
    "react-test-renderer": "16.7.0",
    "typescript": "3.3.3",
    "identity-obj-proxy": "3.0.0"
  }
}